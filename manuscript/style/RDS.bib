@article{aalen_nonparametric_1978,
  title = {Nonparametric {{Inference}} for a {{Family}} of {{Counting Processes}}},
  volume = {6},
  copyright = {Copyright \textcopyright{} 1978 Institute of Mathematical Statistics},
  issn = {0090-5364},
  doi = {10.2307/2958850},
  abstract = {Let B = (N1, \ensuremath{\cdots}, Nk) be a multivariate counting process and let Ft be the collection of all events observed on the time interval [ 0, t]. The intensity process is given by \$\ensuremath{\backslash}Lambda\_i(t) = \ensuremath{\backslash}lim\_\{h \ensuremath{\backslash}downarrow 0\} \ensuremath{\backslash}frac\{1\}\{h\}E(N\_i(t + h) - N\_i(t) \ensuremath{\backslash}mid \ensuremath{\backslash}mathscr\{F\}\_t)\ensuremath{\backslash}quad i = 1, \ensuremath{\backslash}cdots, k.\$ We give an application of the recently developed martingale-based approach to the study of N via \ensuremath{\Lambda}. A statistical model is defined by letting \ensuremath{\Lambda}i(t) = \ensuremath{\alpha}i(t)Yi(t), i = 1, \ensuremath{\cdots}, k, where \ensuremath{\alpha} = (\ensuremath{\alpha}1, \ensuremath{\cdots}, \ensuremath{\alpha}k) is an unknown nonnegative function while Y = (Y1, \ensuremath{\cdots}, Yk), together with N, is a process observable over a certain time interval. Special cases are time-continuous Markov chains on finite state spaces, birth and death processes and models for survival analysis with censored data. The model is termed nonparametric when \ensuremath{\alpha} is allowed to vary arbitrarily except for regularity conditions. The existence of complete and sufficient statistics for this model is studied. An empirical process estimating \ensuremath{\beta}i(t) = \ensuremath{\int}t 0 \ensuremath{\alpha}i(s) ds is given and studied by means of the theory of stochastic integrals. This empirical process is intended for plotting purposes and it generalizes the empirical cumulative hazard rate from survival analysis and is related to the product limit estimator. Consistency and weak convergence results are given. Tests for comparison of two counting processes, generalizing the two sample rank tests, are defined and studied. Finally, an application to a set of biological data is given.},
  timestamp = {2016-06-21T07:31:36Z},
  number = {4},
  urldate = {2012-11-12},
  journal = {The Annals of Statistics},
  author = {Aalen, Odd},
  month = jul,
  year = {1978},
  pages = {701--726},
  file = {Aalen_1978_Nonparametric Inference for a Family of Counting Processes.pdf:/home/johnros/.zotero/zotero/66g0wvis.default/zotero/storage/ZGSKXJQ3/Aalen_1978_Nonparametric Inference for a Family of Counting Processes.pdf:application/pdf}
}

