  month = sep,
  year = {2009},
  keywords = {Adolescent,Adult,Age Distribution,Cause of Death,Cocaine-Related Disorders,complications/epidemiology,complications/epidemiology/mortality,Confidence Intervals,Crack Cocaine,epidemiology,epidemiology/etiology,Female,Great Britain,Hepatitis B,Hepatitis C,HIV Infections,Homeless Persons,Humans,Intravenous,Male,Middle Aged,mortality,Overdose,Prevalence,Primary Health Care,Public Health,Risk Factors,statistics // numerical data,Substance Abuse,Young Adult},
